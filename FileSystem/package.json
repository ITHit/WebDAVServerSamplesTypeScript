{
    "name":  "ithit.webdav.server.filesystem",
    "version":  "1.1.201",
    "description":  "A cross-platform WebDAV Node.js server that runs on Windows, Mac OS X and Linux. This example is a fully-functional WebDAV server that can be used to open, edit and save Microsoft Office documents directly to server, without download/upload steps.",
    "author":  "IT Hit, Ltd",
    "license":  "IT Hit LTD.",
    "dependencies":  {
                         "@types/mime-types":  "^2.1.0",
                         "@types/node":  "^10.7.0",
                         "@types/ws":  "^6.0.1",
                         "@types/xmldom":  "^0.1.27",
                         "ithit.webdav.server":  "^1.0.6",
                         "mime-types":  "^2.1.20",
                         "npm-platform-dependencies":  "^0.1.0",
                         "typescript":  "^3.0.1",
                         "ws":  "^6.1.0",
                         "xml-writer":  "^1.7.0",
                         "xmldom":  "^0.1.27"
                     },
    "linuxDependencies":  {
                              "fs-xattr":  "^0.2.1"
                          },
    "darwinDependencies":  {
                               "fs-xattr":  "^0.2.1"
                           },
    "scripts":  {
                    "postinstall":  "npm install webdav.client --prefix wwwroot/js && node node_modules/npm-platform-dependencies"
                },
    "repository":  {
                       "type":  "git",
                       "url":  "https://github.com/ITHit/WebDAVServerSamplesTypeScript"
                   },
    "config":  {
                   "repositoryPath":  "App_Data/Storage"
               }
}
